{
  "name": "tkaq-frontend",
  "version": "0.0.1",
  "description": "Front end for Trondheim Kommune Air Quality",
  "main": "dist",
  "productName": "Trondheim Kommune Air Quality",
  "bundlesize": [
    {
      "path": "./dist/app.*.js",
      "maxSize": "175 kB"
    },
    {
      "path": "./dist/*chunk*.js",
      "maxSize": "75 kB"
    }
  ],
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js|json|less|css|ts|ts|md}": [
      "jest --findRelatedTests --passWithNoTests",
      "prettier --fix",
      "git add"
    ]
  },
  "scripts": {
    "server": "npm run server:dev:hmr --",
    "start": "nps",
    "test": "nps t"
  },
  "dependencies": {
    "@telenorfrontend/tn-components": "^0.1.2",
    "aurelia-bootstrapper": "2.1.1",
    "aurelia-dialog": "^2.0.0-rc.5",
    "aurelia-framework": "^1.3.0",
    "aurelia-http-client": "1.3.0",
    "aurelia-polyfills": "^1.3.0",
    "aurelia-templating": "^1.8.2",
    "blueimp-md5": "2.10.0",
    "date-fns": "2.0.0-alpha.25",
    "flatpickr": "4.5.2",
    "js-cookie": "^2.2.0",
    "leaflet": "^1.4.0",
    "leaflet-fullscreen": "^1.0.2",
    "leaflet.heat": "^0.2.0",
    "normalize.css": "^8.0.0",
    "nprogress": "0.2.0",
    "plotly.js": "^1.47.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.1.0",
    "@exploratoryengineering/data-mapper-chain": "^0.7.1",
    "@telenorfrontend/eslint-config-telenor": "^0.1.3",
    "@telenorfrontend/stylelint-config-telenor": "^0.1.3",
    "@telenorfrontend/tn-conventions": "^0.6.3",
    "@telenorfrontend/tslint-config-telenor": "^0.1.0",
    "@types/jest": "24.0.6",
    "@types/js-cookie": "^2.2.0",
    "@types/leaflet": "^1.4.3",
    "@types/node": "8.0.17",
    "@types/nprogress": "0.0.29",
