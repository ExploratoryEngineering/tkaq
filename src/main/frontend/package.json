    "aurelia-tools": "^2.0.0",
    "aurelia-webpack-plugin": "3.0.0",
    "autoprefixer": "9.1.3",
    "babel-eslint": "^7.2.3",
    "bundlesize": "^0.17.0",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "5.0.1",
    "css-loader": "^2.1.1",
    "del-cli": "^1.1.0",
    "eslint": "^4.3.0",
    "eslint-config-prettier": "^4.0.0",
    "expose-loader": "0.7.3",
    "extract-text-webpack-plugin": "^3.0.2",
